<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<listeners>
		<listener
			class-name="swagLabsAbstractData.SwagLabsListeners"></listener>
	</listeners>        
	<test thread-count="5" name="NegativeTest">
		<classes>
			<class name="swagLabsPagesTests.SwagLabsNegativeTest" />
		</classes>
	</test>
	<test thread-count="5" name="StandAlonTest">
		<classes>
			<class name="swagLabsPagesTests.SwagLabsStandAlonPracticeTest" />
		</classes>
	</test>
	<test thread-count="5" name="ErrorValidationTest">
		<classes>
			<class name="swagLabsPagesTests.ErrorValidationsTest" />
		</classes>
	</test>
	<test thread-count="5" name="PositiveTest">
		<classes>
			<class name="swagLabsPagesTests.SwagLabsPositiveTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
